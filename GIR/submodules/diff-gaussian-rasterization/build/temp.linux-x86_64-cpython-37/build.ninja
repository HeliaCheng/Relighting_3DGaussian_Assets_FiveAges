ninja_required_version = 1.3
cxx = g++-9
nvcc = /usr/local/cuda-11.6/bin/nvcc

cflags = -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/TH -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda-11.6/include -I/data/chengwr/anaconda3/envs/gir/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/TH -I/data/chengwr/anaconda3/envs/gir/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda-11.6/include -I/data/chengwr/anaconda3/envs/gir/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -I/data/chengwr/code/GIR_ori/submodules/diff-gaussian-rasterization/third_party/glm/ -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=compute_80 -gencode=arch=compute_80,code=sm_80 -ccbin gcc-9 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



